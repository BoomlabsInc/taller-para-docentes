library(tidyverse)
library(datos)


au <- millas[millas$fabricante == "audi", ]
tb <- table(au$modelo)
gr <- data.frame(
  modelo = names(tb),
  n = as.integer(tb)
  )
gr 

millas %>%
  filter(fabricante == "audi") %>%
  count(modelo)


